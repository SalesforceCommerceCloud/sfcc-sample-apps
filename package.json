{
  "name": "sfra-next-app",
  "version": "0.0.1",
  "main": "scripts/start.js",
  "bin": {
    "generateTemplateResources": ".talon/talon-compiler/src/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "jest",
    "build:components": "generateTemplateResources .",
    "build:resources": "node scripts/copy-resources.js",
    "build:lib": "rollup -c ./libs/rollup.libs.config.js",
    "build": "npm run build:lib; npm run build:components && npm run build:resources",
    "start": "node scripts/start.js",
    "plan": "rm -rf node_modules && terraform init terraform/ && terraform plan -var package_name=$npm_package_name -out=my-tfplan terraform/",
    "deploy": "terraform apply \"my-tfplan\"",
    "destroy": "terraform destroy"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
